{
  "name": "craft-web",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "apps/craft-web/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/craft-web",
        "index": "apps/craft-web/src/index.html",
        "main": "apps/craft-web/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/craft-web/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          {
            "glob": "favicon.ico",
            "input": "apps/craft-web/public",
            "output": "/"
          },
          {
            "glob": "**/*",
            "input": "apps/craft-web/src/assets",
            "output": "/assets"
          },
          {
            "glob": "**/*",
            "input": "apps/craft-web/public",
            "output": "/"
          }
        ],
        "styles": [
          "apps/craft-web/src/styles/styles.scss",
          "node_modules/@ionic/angular/css/core.css",
          "node_modules/@ionic/angular/css/normalize.css",
          "node_modules/@ionic/angular/css/structure.css"
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "3mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "3kb",
              "maximumError": "6kb"
            }
          ],
          "outputHashing": "all",
          "fileReplacements": [
            {
              "replace": "apps/craft-web/src/environments/environment.ts",
              "with": "apps/craft-web/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "extractLicenses": true,
          "sourceMap": false,
          "namedChunks": false,
          "aot": true,
          "vendorChunk": false,
          "buildOptimizer": true
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true,
          "aot": false,
          "vendorChunk": true,
          "buildOptimizer": false
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "options": {
        "buildTarget": "craft-web:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "craft-web:build:production"
        },
        "development": {
          "buildTarget": "craft-web:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "craft-web:build"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/craft-web/src/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/craft-web/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "craft-web:build",
        "port": 4200,
        "staticFilePath": "dist/apps/craft-web/browser",
        "spa": true
      }
    }
  }
}
