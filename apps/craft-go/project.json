{
    "name": "craft-go",
    "type": "application",
    "root": "apps/craft-go",
    "sourceRoot": "apps/craft-go",
    "targets": {
      "build": {
        "executor": "nx:run-commands",
        "options": {
          "command": "go build -o dist/craft-go ./..."
        }
      },
      "serve": {
        "executor": "nx:run-commands",
        "options": {
          "command": "go run main.go"
        }
      },
      "test": {
        "executor": "nx:run-commands",
        "options": {
          "command": "go test ./..."
        }
      }
    }
  }
  